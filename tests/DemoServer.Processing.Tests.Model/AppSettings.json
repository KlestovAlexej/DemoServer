        "MinimumLevel": {
            "Default": "Debug"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "rollingInterval": "Day",
                    "outputTemplate": "{Timestamp} ({ThreadId}) [{Level}] {SourceContext} - {Message} {Properties:lj}{NewLine}{Exception}",
